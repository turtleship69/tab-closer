{
    "manifest_version": 2,
    "name": "Move Tabs to New Window",
    "version": "1.0",
    "permissions": [
        "tabs",
        "windows"
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    }
}